    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build Debug",
            "program": "${workspaceFolder}/bin/Debug/net8.0/GenDash.exe",
            "args": [
                "-patterns", "patterns.xml",
                //"-playback","11012803774781948193",
                //"-playspeed","800",
                "-seed", "10",
                "-minmove","20",
                "-maxmove","60",
                "-tasks","10",
                "-maxtime", "1000"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "externalTerminal"
        }
    ]
}